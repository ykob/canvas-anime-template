- 
  var title_merge = "";
  var keywords_merge = "";

  if (sketch.title != "")
    title_merge = sketch.title + " â”‚ " + meta.website_name;
  else
    title_merge = meta.website_name

  if (sketch.keywords != "")
    keywords_merge = sketch.keywords + ", " + meta.keywords;
  else
    keywords_merge = meta_keywords_base;

meta(charset="utf-8")
meta(http-equiv="X-UA-Compatible", content="IE=edge")
title #{title_merge}
meta(name="viewport", content="width=device-width,initial-scale=1,maximum-scale=1,minimum-scale=1")
meta(name="description", content = sketch.description)
meta(name="keywords", content = keywords_merge)
meta(name="theme-color", content="#ffffff")
meta(name="msapplication-TileImage", content="/img/common/ms_tileimage.png")
meta(name="msapplication-TileColor", content="#ffffff")
link(rel="shortcut icon" href="/img/common/icon.ico")
link(rel="apple-touch-icon" href="/img/common/app-icon.ico")
link(rel="stylesheet" href="/css/main.css")

meta(property="og:title", content = title_merge)
meta(property="og:site_name", content = meta.website_name)
meta(property="og:type", content="website")
meta(property="og:description", content = sketch.ogp_description)
meta(property="og:url", content = meta.domain + sketch.path)
meta(property="og:image", content = meta.domain + '/img/common/' + sketch.ogp_img)

meta(name="twitter:card", content="summary_large_image")
meta(name="twitter:title", content = title_merge)
meta(name="twitter:description", content = sketch.ogp_description)
meta(name="twitter:image", content = meta.domain + '/img/common/' + sketch.ogp_img)
